{
  "name": "url-shortener-be",
  "version": "0.1.0",
  "description": "url-shortener-be",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "type": "commonjs",
  "config": {
    "dir": {
      "bin": "./bin",
      "coverage": "./coverage",
      "dist": "./dist",
      "src": "./src"
    }
  },
  "scripts": {
    "start:dev": "nest start --watch -- -b swc",
    "start:dev:docker": "docker compose up --build",
    "start:debug": "nest start --debug --watch",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --no-cache --runInBand .unit. --coverage",
    "test:e2e": "jest --no-cache --runInBand .e2e.",
    "test:dev": "jest --no-cache --runInBand --watch",
    "staged:compile": "npm run compile",
    "staged:lint": "lint-staged",
    "staged:test": "jest --no-cache --runInBand --onlyChanged",
    "format": "prettier **/* --write --ignore-unknown",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:errors": "eslint \"{src,apps,libs,test}/**/*.ts\" --quiet",
    "compile": "tsc --noEmit",
    "build": "nest build",
    "prepare": "husky",
    "commit": "$npm_package_config_dir_bin/git/commit",
    "npm:barrelsby": "barrelsby --config=barrelsbyrc.json",
    "npm:changelog": "conventional-changelog --preset=angular --infile=CHANGELOG.md --same-file --release-count=0",
    "npm:lock": "npm shrinkwrap",
    "npm:publish": "npm publish",
    "npm:check:updates": "ncu --interactive --format group",
    "npm:check:security": "npm audit"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.6",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "joi": "^17.13.3",
    "mongoose": "^8.10.0",
    "nestjs-pino": "^4.1.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "reflect-metadata": "^0.2.0",
    "rotating-file-stream": "^3.2.5",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-angular": "^19.5.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.2",
    "@nestjs/testing": "^10.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "barrelsby": "^2.8.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-custom-rules": "file:eslint-plugin-custom-rules",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "npm-check-updates": "^17.1.8",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "overrides": {}
}
